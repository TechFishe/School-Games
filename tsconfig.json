{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@astroBlocks/*": ["src/components/astro/*"],
      "@reactBlocks/*": ["src/components/react/*"]
    }
  }
}